public class MyClassStudent {

    public static void main(String[] args) {
        ClassStudent ClassStudent1 = new ClassStudent("Martin", 22, 101, 80);
        ClassStudent ClassStudent2 = new ClassStudent("Jobelle", 23, 102, 85);
        ClassStudent ClassStudent3 = new ClassStudent("Alexandria", 24, 103, 90);
        ClassStudent ClassStudent4 = new ClassStudent("Shareimane", 25, 104, 95);
        ClassStudent ClassStudent5 = new ClassStudent("Samuel", 26, 105, 30);
    }
}

    class ClassStudent {
        private String name;
        private int age;
        private int rollno;
        private int marks;

        public ClassStudent(String name, int age, int rollno, int marks) {
            if (marks <= 30) {
                System.out.println("Marks less than 30 cannot be accepted.");
            }
            else {
                this.marks = marks;
                this.name = name;
                this.age = age;
                this.rollno = rollno;
            }
        }
    }
